{
  "name": "donibanevoluntarios",
  "version": "0.0.1",
  "author": "Silvia Ayerra Eusa",
  "private": true,
  "scripts": {
    "cordova": "cordova",
    "cordova:add:android": "cordova platform add android",
    "cordova:rm:android": "cordova platform rm android",
    "cordova:build:android": "ionic cordova build android",
    "cordova:add:ios": "cordova platform add ios",
    "cordova:rm:ios": "cordova platform rm ios",
    "cordova:run:ios": "cordova run ios --target=\"iPhone-XS, com.apple.CoreSimulator.SimRuntime.iOS-12-4\" --buildFlag=\"-UseModernBuildSystem=0\"",
    "cordova:run:android": "cordova run android",
    "cordova:zip": "gulp cordovaZip",
    "ionic:build:test": "ionic cordova build android --prod",
    "ionic:live:ios": "ionic cordova emulate ios --livereload -- --buildFlag=\"-UseModernBuildSystem=0\"",
    "ionic": "ionic --no-interactive",
    "resources": "ionic cordova resources",
    "ionic:emulate:android": "ionic cordova run android --no-interactive --emulator",
    "ionic:run:android": "ionic cordova run android --no-interactive --device",
    "ionic:run:ios": "ionic cordova run ios --no-interactive --device",
    "serve": "ionic serve --no-interactive",
    "serve:old": "ionic serve --no-interactive",
    "serve:real": "ionic serve --no-interactive",
    "build:prod": "ionic-app-scripts lint && ionic-app-scripts build --prod && gulp cordovaZip",
    "build:pds": "ionic-app-scripts lint && ionic-app-scripts build --prod && gulp build:after && gulp removeCordova",
    "build:pds_old": "ionic-app-scripts lint && ionic-app-scripts build && gulp build:after",
    "test": "karma start ./tests/karma.conf.js",
    "test:ci": "karma start ./tests/karma.conf.js --single-run",
    "templates": "gulp templates",
    "test:coverage": "karma start ./tests/karma.conf.js --coverage",
    "translations:extract": "ngx-translate-extract --input ./src --output ./src/core/i18n/translations/template.json --format=json --clean -sort --marker extract",
    "translations:update": "ngx-translate-extract -i ./src -o ./src/core/i18n/translations/eu.json --clean --marker extract && gulp addManualTranslations && gulp orderTranslations",
    "translations:export": "npm run translations:update && gulp exportPendingTranslations",
    "repairCRLF_CS": "gulp repairCRLF_CS"
  },
  "config": {
    "ionic_bundler": "webpack",
    "ionic_webpack": "./config/webpack.config.js"
  },
